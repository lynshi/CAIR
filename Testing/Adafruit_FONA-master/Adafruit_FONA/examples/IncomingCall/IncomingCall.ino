//  }
//  else {
//    Serial.println(F("Caller id notification disabled"));
//  }
}

void loop(){
  char command = Serial.read();
  if(command == 'c')
  {
    flushSerial();
    Serial.println();
    Serial.print(F("Calling ")); Serial.println(number);
    if (!fona.callPhone(number)) {
      Serial.println(F("Failed"));
    } 
    else {
      Serial.println(F("Sent!")); 
    }
    delay(3000);
    while(fona.getCallStatus() != 4)
    {
      Serial.print(F("."));
      delay(500);
    }
    delay(5000);
    startPlayback(zero, sizeof(zero));
    delay(1000);
    stopPlayback();
    delay(5000);
    fona.hangUp();
  }
}

void flushSerial() {
  while (Serial.available())
    Serial.read();
}
